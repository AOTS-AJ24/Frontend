             </tr>

             <!--  Learning colspan  -->
            <tr>
                <td colspan="2" style="height: 20px;"> i have 2 columns</td>
                <td colspan="1" style="font-weight: bold;"> i have 1 </td>
            </tr>

            <!-- ------------------------------------------------------------------ -->

            <tr>
                <td colspan="3" style="height: 30px;"></td>
             </tr>

            <!--------------------------------------------------------------------  -->
